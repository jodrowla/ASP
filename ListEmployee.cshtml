@page
@model ListEmployeeModel

<h2>List of Employees</h2>




<table class="table">
    <thead>
        <tr>
            <th>Employee ID</th>
            <th>Last Name</th>
            <th>First Name</th>
            <th>Title</th>
        </tr>
    </thead>
    <tbody>
   @foreach (var employee in Model.employee)
    {
        <tr>
            <td>@employee.EmployeeId</td>
            <td>@employee.FirstName</td>
            <td>@employee.LastName</td>
            <td>@employee.Title</td>
        </tr>
    }
    </tbody>
</table>